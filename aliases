alias g="git"
alias lg="lazygit"
alias gx="grep '=\"g' $ZDOTDIR/.zshrc | sed -e 's/a //g'"
alias gg="g commit -m 'Update' && g push"
alias grm="g rm --cached"
alias gco="g commit"
alias gr="g restore"
alias gre="g reset"
alias gcl="g clone"
alias gs="g status"
alias gpu="g push"
alias gd="g diff"
alias ga="g add"
